# @version ^0.3.4

@external
@view
def add_int128(a: int128, b: int128) -> int128:
    return a + b

@external
@view
def sub_int128(a: int128, b: int128) -> int128:
    return a - b

@external
@view
def mul_int128(a: int128, b: int128) -> int128:
    return a * b

@external
@view
def div_int128(a: int128, b: int128) -> int128:
    return a / b

@external
@view
def mod_int128(a: int128, b: int128) -> int128:
    return a % b
